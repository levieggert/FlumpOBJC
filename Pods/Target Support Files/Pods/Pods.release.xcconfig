GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Sparrow"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Sparrow"
OTHER_LDFLAGS = -ObjC -l"Pods-Sparrow" -l"z" -framework "AVFoundation" -framework "AudioToolbox" -framework "GLKit" -framework "OpenAL" -framework "OpenGLES" -framework "QuartzCore"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods